<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<Operation Class="PatchOperationRemove">
		<xpath>Defs/ThingDef[defName="BioferriteShaper" or defName="SerumCentrifuge"]/constructEffect</xpath>
	</Operation>

	<Operation Class="SK.PatchOperationReplaceExtended">
		<xpath>Defs/ThingDef[defName="BioferriteShaper"]</xpath>
		<value>
			<stuffCategories>
			  	<li>RuggedMetallic</li>
			</stuffCategories>
			<costStuffCount>120</costStuffCount>
			<costList>
				<Mechanism>8</Mechanism>
				<ElectronicComponents>5</ElectronicComponents>
				<ComponentIndustrial>12</ComponentIndustrial>
			</costList>
		</value>
	</Operation>


	<Operation Class="SK.PatchOperationReplaceExtended">
		<xpath>Defs/ThingDef[defName="SerumCentrifuge"]</xpath>
		<value>
			<stuffCategories>
			  	<li>RuggedMetallic</li>
				<li>StrongMetallic</li>
			</stuffCategories>
			<costStuffCount>100</costStuffCount>
			<costList>
				<Plastic>20</Plastic>
				<Rubber>15</Rubber>
				<ReinforcedGlass>15</ReinforcedGlass>
				<Shard>1</Shard>
				<ComponentIndustrial>2</ComponentIndustrial>
				<BioferriteAlloy>80</BioferriteAlloy>
			</costList>
		</value>
	</Operation>

	<Operation Class="SK.PatchOperationReplaceExtended">
		<xpath>Defs/ThingDef[defName="BioferriteHarvester"]</xpath>
		<value>
			<stuffCategories>
			  	<li>RuggedMetallic</li>
				<li>StrongMetallic</li>
			</stuffCategories>
			<costStuffCount>40</costStuffCount>
			<costList>
				<Wire>10</Wire>
				<Pipe>2</Pipe>
				<Mechanism>1</Mechanism>
				<ElectronicComponents>1</ElectronicComponents>
				<ComponentIndustrial>2</ComponentIndustrial>
			</costList>
		</value>
	</Operation>


</Patch>