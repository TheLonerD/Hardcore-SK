<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<Operation Class="PatchOperation.FindModByID">
		<mods>
			<li>thelonetec.tsap</li>
		</mods>
		<nomatch Class="PatchOperationAdd">
			<xpath>Defs</xpath>
			<value>
				<ArchitectSense.DesignationSubCategoryDef>
					<defName>Walls</defName>
					<label>Walls</label>
					<description>All the main walls from HSK.</description>
					<defNames>
						<li>Wall</li>
						<li>Window</li>
						<li>ClutterSilverWall</li>
						<li>ClutterUWallLight</li>
					</defNames>
					<designationCategory>Structure</designationCategory>
					<debug>false</debug>
				</ArchitectSense.DesignationSubCategoryDef>

				<ArchitectSense.DesignationSubCategoryDef>
					<defName>Doors</defName>
					<label>Doors</label>
					<description>All the main doors from HSK.</description>
					<defNames>
						<li>Door</li>
						<li>MDoor</li>
						<li>DoorGlass</li>
						<li>AnimalFlap</li>
						<li>OrnateDoor</li>
					</defNames>
					<designationCategory>Structure</designationCategory>
					<debug>false</debug>
				</ArchitectSense.DesignationSubCategoryDef>

				<ArchitectSense.DesignationSubCategoryDef>
					<defName>AutoDoors</defName>
					<label>AutoDoors</label>
					<description>All the main autoDoors from HSK.</description>
					<defNames>
						<li>Autodoor</li>
						<li>Blastdoor</li>
						<li>AutoDoorGlass</li>
						<li MayRequire="ludeon.rimworld.anomaly">SecurityDoor</li>
					</defNames>
					<designationCategory>Structure</designationCategory>
					<debug>false</debug>
				</ArchitectSense.DesignationSubCategoryDef>

				<ArchitectSense.DesignationSubCategoryDef>
					<defName>Columns</defName>
					<label>Columns</label>
					<description>Various Columns.</description>
					<defNames>
						<li>Column</li>
					</defNames>
					<designationCategory>Structure</designationCategory>
					<debug>false</debug>
				</ArchitectSense.DesignationSubCategoryDef>
			</value>
		</nomatch>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ArchitectSense.DesignationSubCategoryDef[defName="AutoDoors"]/defNames</xpath>
					<value>
						<li MayRequire="ludeon.rimworld.anomaly">SecurityDoor</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ArchitectSense.DesignationSubCategoryDef[defName="Doors"]/defNames</xpath>
					<value>
						<li>OrnateDoor</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>